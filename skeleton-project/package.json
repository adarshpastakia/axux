{
  "name": "axux-project",
  "version": "0.0.1",
  "description": "AX/UX skeleton project",
  "author": "@authorname",
  "license": "MIT",
  "private": true,
  "type": "module",
  "targets": {
    "project": {
      "source": "public/index.html",
      "context": "browser",
      "scopeHoist": false
    }
  },
  "scripts": {
    "start": "parcel --target project -p 3000",
    "lint:css": "stylelint packages/**/*.css",
    "lint:ts": "eslint --no-error-on-unmatched-pattern .",
    "check:ts": "tsc --noEmit true -p ./tsconfig.json",
    "lint": "npm-run-all lint:css lint:ts check:ts",
    "prebuild": "npm run lint",
    "build": "parcel build --no-cache --target project --dist-dir build",
    "pretest": "npm run lint",
    "test": "jest --ci --runInBand --detectOpenHandles --forceExit",
    "clean": "rm -rf ./.parcel-cache && rm -rf ./build && rm -rf ./dist"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  },
  "dependencies": {
    "@axux/core": "^4.0.0",
    "@axux/utilities": "^4.0.0",
    "@mdi/js": "^7.4.47",
    "react": "next",
    "react-dom": "next",
    "react-router": "^6.22.3",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-modules-commonjs": "^7.24.1",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@parcel/bundler-experimental": "^2.7.0",
    "@parcel/compressor-brotli": "^2.9.1",
    "@parcel/compressor-gzip": "^2.9.1",
    "@parcel/packager-raw-url": "^2.9.1",
    "@parcel/transformer-webmanifest": "^2.9.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "react-router": "^6.22.0",
    "react-router-dom": "^6.22.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "autoprefixer": "^10.4.18",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-css": "^6.0.1",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^24.0.0",
    "jsdom-global": "^3.0.2",
    "lerna": "^8.1.2",
    "lerna-changelog": "^2.2.0",
    "mutationobserver-shim": "^0.3.7",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.9.1",
    "parcel-plugin-externals": "^0.5.2",
    "postcss": "^8.4.35",
    "postcss-import": "^16.0.1",
    "postcss-nesting": "^12.1.0",
    "prettier": "^3.2.5",
    "prop-types": "^15.8.1",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "stylelint-selector-bem-pattern": "^4.0.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.2",
    "web-vitals": "^3.3.1"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  }
}
