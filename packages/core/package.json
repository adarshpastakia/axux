{
  "name": "@axux/core",
  "version": "0.2.11",
  "description": "AxUx: Core package",
  "author": "Adarsh Pastakia <adarshpastakia@outlook.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "directories": {
    "src": "src",
    "test": "__tests__"
  },
  "files": [
    "src",
    "dist",
    "sass"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build:sass": "node-sass sass -o dist",
    "build:lib": "tsc -p ./tsconfig.build.json",
    "prebuild": "rm -rf ./dist",
    "build": "npm-run-all build:sass build:lib",
    "watch": "yarn build:lib -w"
  },
  "dependencies": {
    "@axux/utilities": "^0.2.7",
    "@popperjs/core": "^2.9.3",
    "react-helmet": "^6.1.0",
    "react-popper": "^2.2.5",
    "resize-observer-polyfill": "^1.5.1"
  },
  "peerDependencies": {
    "i18next": "^20.6.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-i18next": "^11.16.2",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-helmet": "^6.1.5",
    "@types/react-i18next": "^8.1.0",
    "@types/react-router-dom": "^5.3.3"
  },
  "engines": {
    "node": ">=10.22.0",
    "npm": ">=7.0.0"
  }
}
